def minion_game(string):
  vowel = 'AIUEO' 
  kevin = stuart = 0
  
  if string.isupper() == True and 0 < len(string) <= 10**6:
      for i in range(len(string)):
          if string[i] not in vowel:
              stuart += len(string) - i
          else:
              kevin += len(string) - i
  
  if kevin > stuart:
      print("Kevin", kevin)
  elif kevin < stuart:
      print("Stuart", stuart)
  else:
      print("Draw")

if __name__ == '__main__':
    s = input()
    minion_game(s)
